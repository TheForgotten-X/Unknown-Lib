        TSBImage.BackgroundTransparency = 1.000
        TSBImage.BorderSizePixel = 0
        TSBImage.Position = UDim2.new(0, 5, 0, 5)
        TSBImage.Size = UDim2.new(0, 30, 0, 30)
        TSBImage.ZIndex = 5
        TSBImage.Image = "rbxassetid://7129850222"
        TSBImage.ImageTransparency = 0.4
        
        TSBIConstant.Name = "TSBI Constant"
        TSBIConstant.Parent = TSBImage
        
        TSBText.Name = "TSB Text"
        TSBText.Parent = TemplateSideButton
        TSBText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TSBText.BackgroundTransparency = 1.000
        TSBText.Position = UDim2.new(0, 41, 0, 5)
        TSBText.Size = UDim2.new(0, 128, 0, 30)
