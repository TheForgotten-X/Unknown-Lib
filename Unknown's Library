            end)
    
            TabDropdownButton.MouseLeave:Connect(function()
                TWS:Create(TDDBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
                TWS:Create(TDDBImage, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{ImageTransparency = 0.4}):Play()
            end)
            
            UIAspectRatioConstraint_10.Parent = TDDBImage
        end

        function ButtonContainer:Slider(Name , Min , Max , Callback)

            local TabSliderMainFrame = Instance.new("Frame")
            local SliderFrame = Instance.new("Frame")
            local SliderButton = Instance.new("TextButton")
            local InnerSlider = Instance.new("Frame")
            local ISCorner = Instance.new("UICorner")
            local SliderAmmount = Instance.new("TextLabel")
            local SAConstant = Instance.new("UIAspectRatioConstraint")
            local SATextConstant = Instance.new("UITextSizeConstraint")
            local SBCorner = Instance.new("UICorner")
            local SBConstant = Instance.new("UIAspectRatioConstraint")
            local SFCorner = Instance.new("UICorner")
            local SFConstant = Instance.new("UIAspectRatioConstraint")
            local TBText_2 = Instance.new("TextLabel")
            local TBTConstant = Instance.new("UIAspectRatioConstraint")
            local TBTTextConstant = Instance.new("UITextSizeConstraint")
            local SMFCorner = Instance.new("UICorner")
            local SMFConstant = Instance.new("UIAspectRatioConstraint")

            if Name == "" then
                Name = "Template"
            end

            TabSliderMainFrame.Name = "Tab Slider MainFrame"
            TabSliderMainFrame.Parent = Tab
            TabSliderMainFrame.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TabSliderMainFrame.BorderSizePixel = 0
            TabSliderMainFrame.Position = UDim2.new(0, 0, 0, 165)
            TabSliderMainFrame.Size = UDim2.new(0, 465, 0, 162)
            TabSliderMainFrame.ZIndex = 3
            
            SliderFrame.Name = "Slider Frame"
            SliderFrame.Parent = TabSliderMainFrame
            SliderFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            SliderFrame.BorderSizePixel = 0
            SliderFrame.Position = UDim2.new(0, 188, 0, 14)
            SliderFrame.Size = UDim2.new(0, 260, 0, 20)
            SliderFrame.ZIndex = 4
            
            SliderButton.Name = "SliderButton"
            SliderButton.Parent = SliderFrame
            SliderButton.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
            SliderButton.BorderSizePixel = 0
            SliderButton.Size = UDim2.new(0, 260, 0, 20)
            SliderButton.ZIndex = 6
            SliderButton.AutoButtonColor = false
            SliderButton.Font = Enum.Font.Code
            SliderButton.Text = ""
            SliderButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            SliderButton.TextSize = 14.000
            
            InnerSlider.Name = "Inner Slider"
            InnerSlider.Parent = SliderButton
            InnerSlider.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
            InnerSlider.BorderSizePixel = 0
            InnerSlider.Position = UDim2.new(-0.000461519696, 0, 0, 0)
            InnerSlider.Size = UDim2.new(0, 0, 0, 20)
            InnerSlider.ZIndex = 7
            
            ISCorner.Name = "IS Corner"
            ISCorner.Parent = InnerSlider
            
            SliderAmmount.Name = "SliderAmmount"
            SliderAmmount.Parent = SliderButton
            SliderAmmount.BackgroundColor3 = Color3.fromRGB(88, 94, 103)
            SliderAmmount.BackgroundTransparency = 1.000
            SliderAmmount.BorderSizePixel = 0
            SliderAmmount.Position = UDim2.new(0, 68, 0, 0)
            SliderAmmount.Size = UDim2.new(0, 120, 0, 20)
            SliderAmmount.ZIndex = 8
            SliderAmmount.Font = Enum.Font.Unknown
            SliderAmmount.Text = Min
            SliderAmmount.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderAmmount.TextScaled = true
            SliderAmmount.TextSize = 15.000
            SliderAmmount.TextWrapped = true
            SliderAmmount.TextTransparency = 0.4

            SAConstant.Name = "SA Constant"
            SAConstant.Parent = SliderAmmount
            SAConstant.AspectRatio = 6.000
            
            SATextConstant.Name = "SA Text Constant"
            SATextConstant.Parent = SliderAmmount
            SATextConstant.MaxTextSize = 12
            
            SBCorner.Name = "SB Corner"
            SBCorner.Parent = SliderButton
            
            SBConstant.Name = "SB Constant"
            SBConstant.Parent = SliderButton
            SBConstant.AspectRatio = 13.000
            
            SFCorner.CornerRadius = UDim.new(0, 12)
            SFCorner.Name = "SF Corner"
            SFCorner.Parent = SliderFrame
            
            SFConstant.Name = "SF Constant"
            SFConstant.Parent = SliderFrame
            SFConstant.AspectRatio = 13.000
            
            TBText_2.Name = "TB Text"
            TBText_2.Parent = TabSliderMainFrame
            TBText_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TBText_2.BackgroundTransparency = 1.000
            TBText_2.Position = UDim2.new(0, 20, 0, 10)
            TBText_2.Size = UDim2.new(0, 180, 0, 30)
            TBText_2.ZIndex = 5
            TBText_2.Font = Enum.Font.Unknown
            TBText_2.Text = Name
            TBText_2.TextColor3 = Color3.fromRGB(255, 255, 255)
            TBText_2.TextScaled = true
            TBText_2.TextSize = 15.000
            TBText_2.TextWrapped = true
            TBText_2.TextXAlignment = Enum.TextXAlignment.Left
            TBText_2.TextTransparency = 0.4

            TabSliderMainFrame.MouseEnter:Connect(function()
                TWS:Create(TBText_2, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
                TWS:Create(SliderAmmount, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
            end)
    
            TabSliderMainFrame.MouseLeave:Connect(function()
                TWS:Create(TBText_2, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
                TWS:Create(SliderAmmount, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
            end)
            
            TBTConstant.Name = "TBT Constant"
            TBTConstant.Parent = TBText_2
            TBTConstant.AspectRatio = 6.000
            
            TBTTextConstant.Name = "TBT Text Constant"
            TBTTextConstant.Parent = TBText_2
            TBTTextConstant.MaxTextSize = 13
            
            SMFCorner.Name = "SMF Corner"
            SMFCorner.Parent = TabSliderMainFrame
            
            SMFConstant.Name = "SMF Constant"
            SMFConstant.Parent = TabSliderMainFrame
            SMFConstant.AspectRatio = 9.300

            local mouse = game.Players.LocalPlayer:GetMouse()
            local Value;

            SliderButton.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(Max) - tonumber(Min)) / 260) * InnerSlider.AbsoluteSize.X) + tonumber(Min)) or 0
                InnerSlider.Size = UDim2.new(0, math.clamp(mouse.X - InnerSlider.AbsolutePosition.X, 0, 260), 0, 20)
                local Move = mouse.Move:Connect(function()
                    SliderAmmount.Text = Value
                    Value = math.floor((((tonumber(Max) - tonumber(Min)) / 260) * InnerSlider.AbsoluteSize.X) + tonumber(Min))
                    InnerSlider.Size = UDim2.new(0, math.clamp(mouse.X - InnerSlider.AbsolutePosition.X, 0, 260), 0, 20)
                    pcall(Callback , Value)
                end)
                local Release = UIS.InputEnded:Connect(function(Mouse)
                    if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(Max) - tonumber(Min)) / 260) * InnerSlider.AbsoluteSize.X) + tonumber(Min))
                        InnerSlider.Size = UDim2.new(0, math.clamp(mouse.X - InnerSlider.AbsolutePosition.X, 0, 260), 0, 20)
                        pcall(Callback , Value)
                    end
                end)
                UIS.InputEnded:Connect(function(Input)
                    if Input.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(Max) - tonumber(Min)) / 260) * InnerSlider.AbsoluteSize.X) + tonumber(Min))
                        pcall(Callback , Value)
                        Move:Disconnect()
                        Release:Disconnect()
                    end
                end)
            end)

        end

        function ButtonContainer:InputBox(Name , Disappear , Callback)

            local TabInput = Instance.new("Frame")
            local TIText = Instance.new("TextLabel")
            local TITConstant = Instance.new("UIAspectRatioConstraint")
            local TITTextConstant = Instance.new("UITextSizeConstraint")
            local TICorner = Instance.new("UICorner")
            local TIConstant = Instance.new("UIAspectRatioConstraint")
            local TIInputBox = Instance.new("TextBox")
            local TIIBCorner = Instance.new("UICorner")
            local TIIBConstrant = Instance.new("UIAspectRatioConstraint")
            local TIIBTextConstant = Instance.new("UITextSizeConstraint")

            if Name == "" then
                Name = "Template"
            end

            TabInput.Name = "Tab Input"
            TabInput.Parent = Tab
            TabInput.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TabInput.BorderSizePixel = 0
            TabInput.Position = UDim2.new(0, 0, 0, 165)
            TabInput.Size = UDim2.new(0, 465, 0, 162)
