            TICorner.Name = "TI Corner"
            TICorner.Parent = TabInput
            
            TIConstant.Name = "TI Constant"
            TIConstant.Parent = TabInput
            TIConstant.AspectRatio = 9.300
            
            TIInputBox.Name = "TI Input Box"
            TIInputBox.Parent = TabInput
            TIInputBox.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
            TIInputBox.BorderSizePixel = 0
            TIInputBox.Position = UDim2.new(0.404301077, 0, 0.300000012, 0)
            TIInputBox.Size = UDim2.new(0, 260, 0, 20)
            TIInputBox.ZIndex = 5
            TIInputBox.Font = Enum.Font.Unknown
            TIInputBox.Text = ""
            TIInputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
            TIInputBox.TextScaled = true
            TIInputBox.TextSize = 14.000
            TIInputBox.TextWrapped = true
            
            TIIBCorner.Name = "TIIB Corner"
            TIIBCorner.Parent = TIInputBox
            
            TIIBConstrant.Name = "TIIB Constrant"
            TIIBConstrant.Parent = TIInputBox
