            TBText.TextScaled = true
            TBText.TextSize = 15.000
            TBText.TextWrapped = true
            TBText.TextXAlignment = Enum.TextXAlignment.Left
            TBText.TextTransparency = 0.4

            TabButton.MouseEnter:Connect(function()
				TWS:Create(TBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
			end)

			TabButton.MouseLeave:Connect(function()
				TWS:Create(TBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
			end)
            
            UIAspectRatioConstraint.Parent = TBText
            UIAspectRatioConstraint.AspectRatio = 6.000
            
            UITextSizeConstraint.Parent = TBText
            UITextSizeConstraint.MaxTextSize = 13
            
            UIAspectRatioConstraint_2.Parent = TabButton
            UIAspectRatioConstraint_2.AspectRatio = 9.300

        end

        function ButtonContainer:ToggleButton(Name , Callback)

            local TabToggleButton = Instance.new("TextButton")
            local TTBCorner = Instance.new("UICorner")
            local TTBText = Instance.new("TextLabel")
            local UIAspectRatioConstraint_3 = Instance.new("UIAspectRatioConstraint")
            local UITextSizeConstraint_2 = Instance.new("UITextSizeConstraint")
            local TTBImage = Instance.new("ImageLabel")
            local UIAspectRatioConstraint_4 = Instance.new("UIAspectRatioConstraint")
            local UIAspectRatioConstraint_5 = Instance.new("UIAspectRatioConstraint")
            local Toggle = false

            if Name == "" then
                Name = "Template"
            end

            TabToggleButton.Name = "Tab Toggle Button"
            TabToggleButton.Parent = Tab
            TabToggleButton.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TabToggleButton.BorderSizePixel = 0
            TabToggleButton.Size = UDim2.new(0, 465, 0, 50)
            TabToggleButton.ZIndex = 4
            TabToggleButton.Font = Enum.Font.SourceSans
            TabToggleButton.Text = ""
            TabToggleButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            TabToggleButton.TextSize = 14.000
            TabToggleButton.AutoButtonColor = false
            TabToggleButton.MouseButton1Click:Connect(function()
                if not Toggle then
                    TTBImage.Image = "rbxassetid://7468883528"
                else
                    TTBImage.Image = "rbxassetid://7468899170"
                end
                Toggle = not Toggle
                pcall(Callback , Toggle)
            end)
            
            TTBCorner.Name = "TTB Corner"
            TTBCorner.Parent = TabToggleButton
            
            TTBText.Name = "TTB Text"
            TTBText.Parent = TabToggleButton
            TTBText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TTBText.BackgroundTransparency = 1.000
            TTBText.Position = UDim2.new(0, 20, 0, 9)
            TTBText.Size = UDim2.new(0, 180, 0, 30)
            TTBText.ZIndex = 5
            TTBText.Font = Enum.Font.Unknown
            TTBText.Text = Name
            TTBText.TextColor3 = Color3.fromRGB(255, 255, 255)
            TTBText.TextScaled = true
            TTBText.TextSize = 15.000
            TTBText.TextWrapped = true
            TTBText.TextXAlignment = Enum.TextXAlignment.Left
            TTBText.TextTransparency = 0.4
            
            UIAspectRatioConstraint_3.Parent = TTBText
            UIAspectRatioConstraint_3.AspectRatio = 6.000
            
            UITextSizeConstraint_2.Parent = TTBText
            UITextSizeConstraint_2.MaxTextSize = 13
            
            TTBImage.Name = "TTB Image"
            TTBImage.Parent = TabToggleButton
            TTBImage.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            TTBImage.BackgroundTransparency = 1.000
            TTBImage.BorderSizePixel = 0
            TTBImage.Position = UDim2.new(0, 409, 0, 5)
            TTBImage.Size = UDim2.new(0, 40, 0, 40)
            TTBImage.ZIndex = 5
            TTBImage.Image = "rbxassetid://7468899170"
            TTBImage.ImageTransparency = 0.4

            TabToggleButton.MouseEnter:Connect(function()
                TWS:Create(TTBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
                TWS:Create(TTBImage, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{ImageTransparency = 0}):Play()
            end)
    
            TabToggleButton.MouseLeave:Connect(function()
                TWS:Create(TTBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
                TWS:Create(TTBImage, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{ImageTransparency = 0.4}):Play()
            end)
            
            UIAspectRatioConstraint_4.Parent = TTBImage
            
            UIAspectRatioConstraint_5.Parent = TabToggleButton
            UIAspectRatioConstraint_5.AspectRatio = 9.300
        end

        function ButtonContainer:Dropdown(Name , Buttons , Size , Callback)

            local TabDropdownButton = Instance.new("TextButton")
            local TDDBCorner = Instance.new("UICorner")
            local TDDBText = Instance.new("TextLabel")
            local TDDBMainframe = Instance.new("ScrollingFrame")
            local UIListLayout = Instance.new("UIListLayout")
            local UIAspectRatioConstraint_8 = Instance.new("UIAspectRatioConstraint")
            local UIAspectRatioConstraint_9 = Instance.new("UIAspectRatioConstraint")
            local UITextSizeConstraint_4 = Instance.new("UITextSizeConstraint")
            local TDDBImage = Instance.new("ImageLabel")
            local UIAspectRatioConstraint_10 = Instance.new("UIAspectRatioConstraint")
            local DropdownToggled = false
            local SelectedItem = ""

            TabDropdownButton.Name = "Tab Dropdown Button"
            TabDropdownButton.Parent = Tab
            TabDropdownButton.Active = false
            TabDropdownButton.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TabDropdownButton.BorderSizePixel = 0
            TabDropdownButton.ClipsDescendants = true
            TabDropdownButton.Position = UDim2.new(0, 0, 0.239130437, 0)
            TabDropdownButton.Size = UDim2.new(0, 465, 0, 50)
            TabDropdownButton.ZIndex = 4
            TabDropdownButton.AutoButtonColor = true
            TabDropdownButton.Font = Enum.Font.SourceSans
            TabDropdownButton.Text = ""
            TabDropdownButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            TabDropdownButton.TextSize = 14.000
            TabDropdownButton.AutoButtonColor = false
            TabDropdownButton.MouseButton1Click:Connect(function()
                if not DropdownToggled then
                    TabDropdownButton:TweenSize(UDim2.new(0, 465, 0, 200), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, .6, true)
                    TWS:Create(TDDBImage, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Rotation = 180}):Play()
                else
                    TabDropdownButton:TweenSize(UDim2.new(0, 465, 0, 50), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, .6, true)
                    TWS:Create(TDDBImage, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Rotation = 0}):Play()
                end
                DropdownToggled = not DropdownToggled
            end)

            if Name == "" then
                Name = "Template"
            end
            
            TDDBCorner.Name = "TDDB Corner"
            TDDBCorner.Parent = TabDropdownButton
            
            TDDBText.Name = "TDDB Text"
            TDDBText.Parent = TabDropdownButton
            TDDBText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TDDBText.BackgroundTransparency = 1.000
            TDDBText.BorderSizePixel = 0
            TDDBText.Position = UDim2.new(0, 20, 0, 9)
            TDDBText.Size = UDim2.new(0, 180, 0, 30)
            TDDBText.ZIndex = 5
            TDDBText.Font = Enum.Font.Unknown
            TDDBText.Text = Name .. " [ " .. "Selected" .. " ]"
            TDDBText.TextColor3 = Color3.fromRGB(255, 255, 255)
            TDDBText.TextScaled = true
            TDDBText.TextSize = 15.000
            TDDBText.TextWrapped = true
            TDDBText.TextXAlignment = Enum.TextXAlignment.Left
            TDDBText.TextTransparency = 0.4
            
            TDDBMainframe.Name = "TDDB Mainframe"
            TDDBMainframe.Parent = TDDBText
            TDDBMainframe.Active = true
            TDDBMainframe.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TDDBMainframe.BorderSizePixel = 0
            TDDBMainframe.Position = UDim2.new(-0.111111112, 0, 1.50362349, 0)
            TDDBMainframe.Size = UDim2.new(0, 465, 0, 140)
            TDDBMainframe.ZIndex = 6
            TDDBMainframe.CanvasSize = UDim2.new(0, 0, Size, 0)
            TDDBMainframe.ScrollBarThickness = 4
            for i, x in ipairs(Buttons) do
                if x == "" then
                    x = "Template"
                end

                local TDDBButton = Instance.new("TextButton")
                local TDDBBCorner = Instance.new("UICorner")
                local TDDBBText = Instance.new("TextLabel")
                local UIAspectRatioConstraint_6 = Instance.new("UIAspectRatioConstraint")
                local UITextSizeConstraint_3 = Instance.new("UITextSizeConstraint")
                local UIAspectRatioConstraint_7 = Instance.new("UIAspectRatioConstraint")

                TDDBButton.Name = "TDDB Button"
                TDDBButton.Parent = TDDBMainframe
                TDDBButton.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
                TDDBButton.BorderSizePixel = 0
                TDDBButton.ClipsDescendants = true
                TDDBButton.Position = UDim2.new(0, 0, 0.0461538471, 0)
                TDDBButton.Size = UDim2.new(0, 465, 0, 50)
                TDDBButton.ZIndex = 7
                TDDBButton.Font = Enum.Font.SourceSans
                TDDBButton.Text = ""
                TDDBButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                TDDBButton.TextSize = 14.000
                TDDBButton.AutoButtonColor = false
                TDDBButton.MouseButton1Click:Connect(function()
                    pcall(Callback , x)
                    SelectedItem = x
                    TDDBText.Text = Name .. " [ " .. SelectedItem .. " ]"
                end)
                
                TDDBBCorner.Name = "TDDBB Corner"
                TDDBBCorner.Parent = TDDBButton
                
                TDDBBText.Name = "TDDBB Text"
                TDDBBText.Parent = TDDBButton
                TDDBBText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                TDDBBText.BackgroundTransparency = 1.000
                TDDBBText.ClipsDescendants = true
                TDDBBText.Position = UDim2.new(0, 20, 0, 9)
                TDDBBText.Size = UDim2.new(0, 180, 0, 30)
                TDDBBText.ZIndex = 8
