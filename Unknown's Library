            if Name == "" then
                Name = "Template"
            end

            TabButton.Name = "Tab Button"
            TabButton.Parent = Tab
            TabButton.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TabButton.BorderSizePixel = 0
            TabButton.Size = UDim2.new(0, 465, 0, 50)
            TabButton.ZIndex = 4
            TabButton.Font = Enum.Font.SourceSans
            TabButton.Text = ""
            TabButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            TabButton.TextSize = 14.000
            TabButton.AutoButtonColor = false
            TabButton.MouseButton1Click:Connect(function()
                pcall(Callback)
            end)
            
            TBCorner.Name = "TB Corner"
            TBCorner.Parent = TabButton
            
            TBText.Name = "TB Text"
            TBText.Parent = TabButton
            TBText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TBText.BackgroundTransparency = 1.000
            TBText.Position = UDim2.new(0, 20, 0, 9)
            TBText.Size = UDim2.new(0, 180, 0, 30)
            TBText.ZIndex = 5
            TBText.Font = Enum.Font.Unknown
            TBText.Text = Name
