print("Executed Unknown's Library")
local Lib = {}

function Lib:Create(Name , Game , Key , DisableKey)

    local UIS = game:GetService("UserInputService")
    local TWS = game:GetService("TweenService")
    
    local UnknownsUILibrary11v = Instance.new("ScreenGui")
    local Mainframe = Instance.new("Frame")
    local MainframeConstant = Instance.new("UIAspectRatioConstraint")
    local SideMenu = Instance.new("Frame")
    local SMDropShadow = Instance.new("Frame")
    local SMDSConstant = Instance.new("UIAspectRatioConstraint")
    local SideMenuButtons = Instance.new("ScrollingFrame")
    local SMList = Instance.new("UIListLayout")
    local SMBConstant = Instance.new("UIAspectRatioConstraint")
    local TabHoldingFrame = Instance.new("Frame")
    local LibNameFrame = Instance.new("Frame")
    local UIName = Instance.new("TextLabel")
    local UNConstant = Instance.new("UIAspectRatioConstraint")
    local UNTextConstant = Instance.new("UITextSizeConstraint")
    local LNFDropShadow = Instance.new("Frame")
    local LNFDSConstant = Instance.new("UIAspectRatioConstraint")
    local UIGameName = Instance.new("TextLabel")
    local UGNConstant = Instance.new("UIAspectRatioConstraint")
    local UGNTextConstant = Instance.new("UITextSizeConstraint")
    local LNFConstant = Instance.new("UIAspectRatioConstraint")
    local SMConstant = Instance.new("UIAspectRatioConstraint")
    local THFConstant = Instance.new("UIAspectRatioConstraint")

    local function DragScript()
        local gui = Mainframe
        local dragging
        local dragInput
        local dragStart
        local startPos
        local function update(input)
            local delta = input.Position - dragStart
            gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
        gui.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = input.Position
                startPos = gui.Position
                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        dragging = false
                    end
                end)
            end
        end)
        gui.InputChanged:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
                dragInput = input
            end
        end)
        UIS.InputChanged:Connect(function(input)
            if input == dragInput and dragging then
                update(input)
            end
        end)
    end
    DragScript()

    if not DisableKey then
        UIS.InputBegan:Connect(function(Input , Chat)
            if not Chat and Input.KeyCode.Name == Key then
                if UnknownsUILibrary11v.Enabled then
                    UnknownsUILibrary11v.Enabled = false
                else
                    UnknownsUILibrary11v.Enabled = true
                end 
            end
        end)
    end

    if Name == "" then
        Name = "Unknown's UI Library"
    end
    if Game == "" then
        Game = "Template"
    end

    UnknownsUILibrary11v.Name = "Unknown's UI Library"
    UnknownsUILibrary11v.Parent = game:GetService("CoreGui")
    
    Mainframe.Name = "Mainframe"
    Mainframe.Parent = UnknownsUILibrary11v
    Mainframe.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    Mainframe.BorderSizePixel = 0
    Mainframe.Position = UDim2.new(0, 562, 0, 254)
    Mainframe.Size = UDim2.new(0, 706, 0, 484)

    MainframeConstant.Name = "Mainframe Constant"
    MainframeConstant.Parent = Mainframe
    MainframeConstant.AspectRatio = 1.459
    
    SideMenu.Name = "SideMenu"
    SideMenu.Parent = Mainframe
    SideMenu.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
