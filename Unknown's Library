            TIIBTextConstant.Name = "TIIB Text Constant"
            TIIBTextConstant.Parent = TIInputBox
            TIIBTextConstant.MaxTextSize = 12

            TabInput.MouseEnter:Connect(function()
                TWS:Create(TIText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
                TWS:Create(TIInputBox, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
            end)
    
            TabInput.MouseLeave:Connect(function()
                TWS:Create(TIText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
                TWS:Create(TIInputBox, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
            end)

            TIInputBox.FocusLost:Connect(function()
                pcall(Callback, TIInputBox.Text)
                if Disappear then
                    TIInputBox.Text = ""
                end
			end)

        end

        function ButtonContainer:TextLabel(Text)
            local TabTextLabel = Instance.new("Frame")
            local TTLText = Instance.new("TextLabel")
            local TTLTConstant = Instance.new("UIAspectRatioConstraint")
            local TTLTTextConstant = Instance.new("UITextSizeConstraint")
            local TTLTextConstant = Instance.new("UICorner")
            local TTLConstant = Instance.new("UIAspectRatioConstraint")

            TabTextLabel.Name = "Tab TextLabel"
            TabTextLabel.Parent = Tab
            TabTextLabel.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            TabTextLabel.BorderSizePixel = 0
