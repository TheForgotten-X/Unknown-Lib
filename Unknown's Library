                TDDBBText.TextWrapped = true
                TDDBBText.TextXAlignment = Enum.TextXAlignment.Left
                TDDBBText.TextTransparency = 0.4

                TDDBButton.MouseEnter:Connect(function()
                    TWS:Create(TDDBBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
                end)
        
                TDDBButton.MouseLeave:Connect(function()
                    TWS:Create(TDDBBText, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
                end)    
                
                UIAspectRatioConstraint_6.Parent = TDDBBText
                UIAspectRatioConstraint_6.AspectRatio = 6.000
                
                UITextSizeConstraint_3.Parent = TDDBBText
                UITextSizeConstraint_3.MaxTextSize = 13
                
                UIAspectRatioConstraint_7.Parent = TDDBButton
                UIAspectRatioConstraint_7.AspectRatio = 9.300
            end
            
            UIListLayout.Parent = TDDBMainframe
            UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            
            UIAspectRatioConstraint_8.Parent = TDDBMainframe
            UIAspectRatioConstraint_8.AspectRatio = 3.321
            
            UIAspectRatioConstraint_9.Parent = TDDBText
            UIAspectRatioConstraint_9.AspectRatio = 6.000
            
            UITextSizeConstraint_4.Parent = TDDBText
            UITextSizeConstraint_4.MaxTextSize = 13
            
            TDDBImage.Name = "TDDB Image"
            TDDBImage.Parent = TabDropdownButton
