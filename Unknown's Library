        TemplateSideButton.MouseButton1Click:Connect(function()
            for i, x in ipairs(TabHoldingFrame:GetChildren()) do
                if x.Name == "Tab" then
                    x.Visible = false
                end
            end
            Tab.Visible = true
        end)
        
        TSBCorner.Name = "TSB Corner"
        TSBCorner.Parent = TemplateSideButton
        
        TSBImage.Name = "TSB Image"
        TSBImage.Parent = TemplateSideButton
